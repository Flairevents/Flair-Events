.v2-interview-check-box [type="checkbox"]:checked,
.v2-interview-check-box [type="checkbox"]:not(:checked) {
    display: none;
}

.v2-interview-check-box [type="checkbox"]:checked + label,
.v2-interview-check-box [type="checkbox"]:not(:checked) + label
{
    position: relative;
    padding-left: 45px;
    padding-top: 5px;
    cursor: pointer;
    line-height: 20px;
    display: inline-block;
    opacity: 1;
    width: 100%;
}
.v2-interview-check-box [type="checkbox"]:checked + label:before,
.v2-interview-check-box [type="checkbox"]:not(:checked) + label:before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 29px;
    height: 29px;
    border-radius: 50%;
    background: #fff;
}
.v2-interview-check-box [type="checkbox"]:checked + label:after,
.v2-interview-check-box [type="checkbox"]:not(:checked) + label:after {
    content: '';
    width: 21px;
    height: 21px;
    background: #008080;
    position: absolute;
    top: 4px;
    left: 4px;
    border-radius: 50%;
    -webkit-transition: all 0.2s ease;
    transition: all 0.2s ease;
}
.v2-interview-check-box [type="checkbox"]:not(:checked) + label:after {
    opacity: 0;
    -webkit-transform: scale(0);
    transform: scale(0);
}
.v2-interview-check-box [type="checkbox"]:checked + label:after {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1);
}
.v2-interview-check-box label {
    text-align: left;
    font: normal normal 500 17px/20px Proxima Nova;
    letter-spacing: 0px;
    color: #FFFFFF;
    opacity: 1;
}

