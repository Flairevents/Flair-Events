.v2-profile-radio [type="radio"]:checked,
.v2-profile-radio [type="radio"]:not(:checked) {
    display: none;
}

.v2-profile-radio [type="radio"]:checked + label,
.v2-profile-radio [type="radio"]:not(:checked) + label
{
    position: relative;
    padding-left: 28px;
    cursor: pointer;
    line-height: 20px;
    display: inline-block;
    color: var(--unnamed-color-ffffff);
    text-align: left;
    font: normal normal 400 14px/19px Proxima Nova;
    letter-spacing: 0px;
    color: #FFFFFF;
    opacity: 1;
}
.v2-profile-radio [type="radio"]:checked + label:before,
.v2-profile-radio [type="radio"]:not(:checked) + label:before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 20px;
    height: 20px;
    border: 1px solid #ddd;
    border-radius: 100%;
    background: #fff;
}
.v2-profile-radio-empty [type="radio"]:checked + label:before,
.v2-profile-radio-empty [type="radio"]:not(:checked) + label:before {
    border: 1px solid red;
}
.v2-profile-radio [type="radio"]:checked + label:after,
.v2-profile-radio [type="radio"]:not(:checked) + label:after {
    content: '';
    width: 12px;
    height: 12px;
    background: #EE7623E5;
    position: absolute;
    top: 4px;
    left: 4px;
    border-radius: 100%;
    -webkit-transition: all 0.2s ease;
    transition: all 0.2s ease;
}
.v2-profile-radio [type="radio"]:not(:checked) + label:after {
    opacity: 0;
    -webkit-transform: scale(0);
    transform: scale(0);
}
.v2-profile-radio [type="radio"]:checked + label:after {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1);
}

