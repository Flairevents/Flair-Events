    color: #FFFFFF;
    opacity: 1;
}

.v2-display-none {
    display: none !important;
}

.v2-navbar-dropdown-content {
    position: absolute;
    z-index: 999;
    width: 180px;
    height: auto;
    /* UI Properties */
    background: #008080 0% 0% no-repeat padding-box;
    border-radius: 2px;
    opacity: 1;
    padding-top: 8px;
    padding-left: 7px;
    padding-bottom: 10px;
    padding-right: 8px;
    top: 70px;
    left: 50%;
    transform: translateX(-50%);
    border-radius: 10px;
    &::after {
        display: inline-block;
        width: 0;
        height: 0;
//        margin-left: .255em;
        vertical-align: .255em;
        content: "";
        border-top: .9em solid #008080;
        border-right: .5em solid transparent;
        border-bottom: 0;
        border-left: .5em solid transparent;
        top: -15px;
        transform: translateX(-50%) rotate(180deg);
        position: absolute;
        left: 50%;
//        box-shadow: 0 2px 5px 2px #00000021;
    }
    &-link {
        transition: transform .4s;
        &:hover {
            transform: translateX(10px);
//            transition: transform .4s;
        }
    }
    &-img {
        height: 20px;
        width: 20px;
        margin-right: 20px;
    }
}

.v2-navbar-dropdown-content a {
    /* UI Properties */
    color: var(--unnamed-color-ffffff);
    text-align: left;
