.v2-jobboard-header-container{
  position: absolute;
  top: 1px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  background: transparent linear-gradient(96deg, #15B9BB66 3%, #FFFFFF00 97%) 0% 0% no-repeat padding-box;
  background-color: #21145F;
  opacity: 1;
  width: 100%;
  height: 620px;
  z-index: -2;
}

.map-bg{
  position: absolute;
  top: 50px;
  left: 562px;
  width: 616px;
  height: 766px;
  opacity: 1;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  background: transparent url("<%= asset_path 'v2/Group 1.png' %>") 0% 0% no-repeat padding-box;
  z-index: 1;
}

.row>*{
  z-index: 1;
}

.overlay{

}

.v2-search-apply {
  text-align: center;
  font: normal normal 700 65px/86px Roboto;
  letter-spacing: 0px;
  opacity: 1;
  padding: 50px 0px 50px;
  color: #FFFFFF;
}

.v2-work{
  text-align: center;
  font: normal normal 700 65px/86px Roboto;
  letter-spacing: 0px;
  opacity: 1;
  color: #6BcBBB;
  display: inline;
}

.v2-search-job input{
  width: 407px;
  height: 42px;
  background: #FFFFFF 0% 0% no-repeat padding-box;
  border-radius: 2px;
  opacity: 1;
  font: normal normal 700 15px/20px Proxima Nova;
  letter-spacing: 0px;
  color: #008080;
  border: 0;
  padding: 10px 10px 10px 10px;
  z-index: 0;
}

.v2-search-job input::placeholder{
  font: italic normal 400 15px/20px Proxima Nova;
  letter-spacing: 0px;
  color: #0080807F;
}

.center{
  margin: auto;
  text-align: center;
}

.mac .v2-find-jobs{
  height: 44px !important;
  padding-top: 3px !important;
  padding-bottom: 0px !important;
}
.v2-search-job .v2-find-jobs{
  width: 149px;
  height: 42px;
  background: #EE7623 0% 0% no-repeat padding-box;
  border: 2px solid #FFFFFF;
  border-radius: 5px;
  opacity: 1;
  text-align: left;
  font: normal normal 700 16px/0px Roboto;
  letter-spacing: 0px;
  color: #FFFFFF;
  text-align: center;
  outline: 0;
  padding-top: 0px;
  padding-bottom: 3px;
}

.v2-search-additional{
  text-align: center;
  font: normal normal 700 40px/53px Proxima Nova;
  letter-spacing: 0px;
  color: #FFFFFF;
  padding: 30px 0px 30px;
}

.v2-search-additional .job-detail{
  display: inline;
  color: #6BCBB8;
}

ul.v2-job-nav{
  height: 137px;
  list-style-type: none;
  list-style: none;
  padding: 0;
  margin: 0;
}

.v2-job-nav li{
  min-width: 81px;
  margin-right: 47px;
  margin-left: 47px;
  display: inline-block;
  text-align: center;
  position: relative;
}

.v2-job-nav li img{
  width: 81px;
  height: 81px;
  margin-bottom: 20px;
  /*border: 1px solid;*/
}

.v2-job-nav li span{
  display: block;
  font: normal normal 700 14px/19px Roboto;
  letter-spacing: 0px;
  color: #FFFFFF;
  opacity: 1;
  margin-bottom: 5px;
}

.v2-job-content{
  /*background: #3B9B90 0% 0% no-repeat padding-box;*/
  opacity: 1;
}

.v2-job-content .btn-all-jobs{
  background: #6BCBB8 0% 0% no-repeat padding-box;
  border: 2px solid #FFFFFF;
  border-radius: 5px;
  text-align: center;
  font: normal normal 700 14px/19px Roboto;
  letter-spacing: 0px;
  color: #FFFFFF;
  opacity: 1;
  width: 136px;
  height: 40px;
  margin-top: 20px;
  position: absolute;
  text-transform: uppercase;
}

.v2-job-content .title-content{
  display: flex;
}

.v2-job-content .latest-jobs-text, .latest-jobs-text2 {
  text-align: center;
  font: normal normal 700 35px/47px Roboto;
  letter-spacing: 0px;
  color: #FFFFFF;
  opacity: 1;
  margin-top: 20px;
  /*margin-left: -136px;*/
  flex:1 0 auto;
}

.v2-job-content .separator-green{
  width: 100%;
  margin-top: 5px;
  margin-bottom: 10px;
  border: 2px solid #6BCBB8;
  opacity: 1;
}

.v2-job-content .job-accordion{
  background: #FFFFFF 0% 0% no-repeat padding-box;
  border-radius: 5px;
  opacity: 1;
  /*cursor: pointer;*/
  padding: 22px 20px;
  width: 100%;
  height: 140px;
  border: none;
  text-align: left;
  outline: none;
  font-size: 15px;
  transition: 0.4s;
}

.v2-job-content .job-accordion.active{
  margin-bottom: 0px;
  border-radius: 5px 5px 0px 0px;
}

.v2-job-content .job-accordion-panel{
  padding: 0 18px;
  display: none;
  overflow: hidden;
  width: 100%;
  /*height: 457px;*/
  /*height: 500px;*/
  height: auto;
  background: #EBFFFB 0% 0% no-repeat padding-box;
  border-radius: 0px 0px 5px 5px;
  opacity: 1;
  /*margin-bottom: 10px;*/
}

.job-accordion-panel .panel-contents{
  padding: 20px 0px;
}

.panel-contents button.job-desc{
  background: #FFFFFF 0% 0% no-repeat padding-box;
  border: 2px solid #008080;
  border-radius: 2px;
  opacity: 1;
  text-align: center ;
  font: normal normal 400 14px/16px Proxima Nova;
  letter-spacing: 0px;
  color: #008080;
  width: 144px;
  height: 30px;
  margin-bottom: 5px;
  outline: 0;
}

.panel-contents button.job-selected{
  background: #008080 0% 0% no-repeat padding-box;
  font: normal normal 400 14px/16px Proxima Nova;
  color: #FFFFFF;
  opacity: 1;
}

.panel-contents label{
  font: normal normal 700 14px/30px Proxima Nova;
  letter-spacing: 0px;
  color: #2F353D;
  opacity: 1;
}

.panel-contents label.normal-text{
  font: normal normal 400 14px/18px Proxima Nova;
}

.panel-contents label.normal-bold-text{
  font: normal normal 700 14px/18px Proxima Nova;
}

.panel-contents label.normal-green-text{
  font: normal normal 400 14px/18px Proxima Nova;
  color: #008080;
  opacity: 1;
}

.panel-contents h1{
  text-align: right;
  font: normal normal 700 35px/18px Proxima Nova;
  letter-spacing: 0px;
  color: #008080;
  opacity: 1;
}
.job-accordion .job-board-mobile-arrow {
    display: flex;
    align-items: center;
    justify-content: center;
}
.job-accordion img{
  width: 135px;
  height: 100%;
  margin-right: 40px;
  object-fit: contain;
  object-position: center;
}

.job-accordion img.job-type{
  opacity: 1;
  width: 50px;
  height: 50px;
  margin: auto;
  margin-bottom: 10px;
}

.job-accordion .event-title{
  text-align: left;
  font: normal normal 700 18px/30px Roboto;
  letter-spacing: 0px;
  color: #6BCBB8;
  opacity: 1;
  width: 400px;
}

.job-accordion .event-info{
  text-align: left;
  width: 400px;
  font: normal normal 400 13px/13px Proxima Nova;
  letter-spacing: 0px;
  color: #2F353D;
  opacity: 1;
}

.job-accordion .job-rate{
  text-align: center;
  font: normal normal 700 15px/20px Proxima Nova;
  letter-spacing: 0px;
  color: #008080;
  opacity: 1;
}

.job-accordion .less-details{
  background: #008080 0% 0% no-repeat padding-box !important;
  border-radius: 5px;
  text-align: center;
  color: #FFFFFF !important;
  border-width: 0px;
}

.job-accordion .view-details {
  background: #FFFFFF 0% 0% no-repeat padding-box;
  border: 2px solid #008080;
  border-radius: 5px;
  text-align: center;
  font: normal normal 700 15px/30px Roboto;
  letter-spacing: 0px;
  color: #008080;
  opacity: 1;
  height: 45px;
  width: 191px;
  outline: 0;
  padding-top: 4px;
}

.job-accordion .apply-job{
  background: #EE7623 0% 0% no-repeat padding-box;
  border-radius: 5px;
  opacity: 1;
  text-align: center;
  font: normal normal 700 15px/30px Roboto;
  letter-spacing: 0px;
  color: #FFFFFF;
  width: 191px;
  border-width: 0px;
  height: 45px;
  padding-top: 4px;
}

.job-accordion .cancel-job{
  background: #EE7623 0% 0% no-repeat padding-box;
  border-radius: 5px;
  opacity: 1;
  text-align: center;
  font: normal normal 700 15px/30px Roboto;
  letter-spacing: 0px;
  color: #FFFFFF;
  width: 191px;
  border-width: 0px;
  height: 45px;
  padding-top: 3px;
}

.job-accordion .separator-orange{
  width: 100%;
  height: 1px;
  background-color: #EE7623;
  margin-top: 5px;
  margin-bottom: 10px;
}

.job-accordion .event-location{
  text-align: left;
  font: normal normal 700 13px/30px Roboto;
  letter-spacing: 0px;
  color: #008080;
  opacity: 1;
  display: flex;
  align-items: center;
}

.job-accordion .rate-range{
  width: 96px;
}

.job-accordion .accordion-buttons{
  width: 191px;
}

.v2-job-content .bottom-more-info{
  text-align: center;
  letter-spacing: 0px;
  font: normal normal 400 15px/30px Proxima Nova;
  color: #FFFFFF;
  opacity: 1;
}

.bottom-more-info .work-with-flair{
  font: normal normal 700 18px/30px Proxima Nova;
}

.bottom-more-info .you-need{
  font: normal normal 600 20px/30px Roboto;
}

.bottom-more-info img{
  width: 28px;
  height: 28px;
  /*border: 1px solid;*/
  margin-right: 15px;
}

