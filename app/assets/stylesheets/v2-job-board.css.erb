.v2-event-month-separator {
  background-color: #6BCBB8;
  padding: 10px;
  color: white;
  text-align: center;
  text-transform: uppercase;
  list-style: none;
  border-radius: 8px;
  margin-top: 15px;
  margin-bottom: 15px;
  width: 100%;
  font: normal normal 700 22px/25px Roboto
}

.v2-job-board-pagination {
  font: normal normal 700 18px/30px Proxima Nova;
  letter-spacing: 0px;
  color: #EE7623;
}

.v2-job-board-pagination a {
  font: normal normal 700 18px/30px Proxima Nova;
  letter-spacing: 0px;
  color: #FFFFFF;
  opacity: 1;
