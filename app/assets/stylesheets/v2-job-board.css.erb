}

/* mobile */
@media only screen and (max-width: 600px){
  .v2-search-apply, .v2-work {
    font: normal normal bold 35px/46px Roboto;
  }
  .job-accordion .job-rate {
    margin-right: 5px;
  }
  .v2-search-additional {
    font: normal normal bold 20px/26px Roboto;
  }

  .v2-job-nav {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    width: 300px;
    margin: 30px auto 0 auto !important;
  }

  .v2-job-nav li{
    min-width: 50px;
    margin-right: 15px;
    margin-left: 15px;
    display: inline-block;
    text-align: center;
    position: relative;
    margin-bottom: 10px;
    width: 60px;
  }

  .v2-job-nav li img {
    width: 50px;
    height: 50px;
    margin-bottom: 5px;
  }

  .v2-job-nav li span {
    font: normal normal normal 10px/13px Roboto;
  }

  .v2-search-job input {
    width: 45%;
    height: 42px;
  }


  .v2-job-board-content-layer-1, .v2-job-board-content-layer-2, .v2-job-board-content-layer-3, .v2-job-board-content-layer-4 {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 700px;
  }

  .v2-job-board-content-layer-1 {
    background: #21145F 0% 0% no-repeat padding-box;
    z-index:-3;
  }

  .v2-job-board-content-layer-2 {
    background: #015F5F73 0% 0% no-repeat padding-box;
    z-index:-2;
  }

  .v2-job-board-content-layer-3 {
    background: #008080C2 0% 0% no-repeat padding-box;
    z-index:-2;
  }

  .v2-job-board-content-layer-4 {
    background: transparent linear-gradient(100deg, #15B9BB66 3%, #FFFFFF00 97%) 0% 0% no-repeat padding-box;
    z-index:-1;
  }

  .map-bg-mobile {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 800px;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
    background: transparent url("<%= asset_path 'v2/Group 1.png' %>") 100% 0% no-repeat padding-box;
    object-fit: cover;
  }

  .latest-jobs-text {
    font: normal normal bold 18px/24px Roboto !important;
  }

  .mobile-job-board-bg {
      background: #3B9B90 0% 0% no-repeat padding-box;
      margin-top: 60px !important;
      position: relative;
      z-index: 9;
  }

  .v2-job-content .job-accordion {
    width: 343px;
    height: auto;
    padding: 5px;
    border-radius: 0 !important;
    position:relative;
    cursor: pointer;
    min-height: 80px;
  }

  .job-accordion img {
    width: 60px;
    height: 60px;
    margin-right: 10px;
  }

  .event-title {
    font: normal normal bold 18px/30px Roboto !important;
    width: fit-content !important;
  }
  .event-title-elip {
      max-width: 170px;
      white-space: nowrap;
      text-overflow: ellipsis;
      overflow: hidden;
  }

  .event-info {
    font: normal normal normal 13px/18px Proxima Nova !important;
    width: 230px !important;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
  }

  .job-board-mobile-arrow {
    width: 25px;
    height: 100%;
    /* UI Properties */
    background: #B3E9DF 0% 0% no-repeat padding-box;
    opacity: 1;
    position: absolute;
    top: 0;
    right: 0;
  }
  .job-board-mobile-arrow img {
    margin-left:9px;
/*    margin-top:35px;*/
    width:auto;
    height:auto;
  }

  .event-location, .job-rate {
    font: normal normal 700 11px/18px Roboto !important;
  }

  .event-location {
    width: auto;
  }

  .job-rate {
    margin-left: 10px;
  }

  .v2-event-month-separator {
    font-size: 18px;
    padding: 5px;
  }

  .panel-contents .mobile-job-buttons button{
    width: 110px;
    height: 40px;
    font: normal normal normal 12px/16px Roboto;
    margin-right: 1px;
    margin-left: 1px;
  }

  .v2-job-content .job-accordion-panel {
    padding: 0;
  }

  .mobile-job-buttons  {
    width: 343px;
    max-width: 343px;
  }

  .mobile-details {
    padding: 5px;
  }

  .mobile-details label, .applied-position {
    width: 100%;
    font: normal normal bold 12px/20px Roboto !important;
    margin-top: 5px !important;
    text-align: left !important;
  }

  .mobile-details div {
    font: normal normal normal 12px/15px Roboto;
    color: #008080;
    text-align: left;
  }

  .panel-contents h1 {
    text-align: left;
    margin-top: 3px;
    font: normal normal 700 33px/25px Proxima Nova;
  }

  .mobile-details div.normal-text {
    margin-top: 3px;
    text-align: left;
    font: normal normal normal 12px/18px Roboto;
    color: black;
  }

  .job-accordion-panel .panel-contents {
    max-width: 343px;
  }

  .v2-job-content .btn-all-jobs {
    width: 90px;
    height: 35px;
    font: normal normal normal 12px/19px Roboto;
    top: 10px;
    left: 30px;
    margin-top: 0;
  }

  .view-details-mobile {
    margin-top: 10px;
    width: 191px;
    height: 45px;
    font: normal normal bold 15px/30px Proxima Nova;
    color: #FFFFFF;

    background: #008080 0% 0% no-repeat padding-box;
    border-radius: 5px;
    opacity: 1;
    border: none;

  }
    @media only screen and (max-width: 600px) {
        .view-details-mobile {
            display: none;
        }
    }
  .apply-job, .cancel-job {
    min-width: 120px;
    height: 30px;
    margin-bottom: 20px;
    font: normal normal bold 15px/30px Roboto;
    margin-top: 10px;
    color: #FFFFFF;

    /* UI Properties */
    background: #EE7623 0% 0% no-repeat padding-box;
    border-radius: 5px;
    opacity: 1;
    border: none;
  }

  .v2-interview-dates-content {
    margin: 15px;
    padding: 20px;
  }

  .prev.v2-job-board-pagination, .next.v2-job-board-pagination {
    margin-right: 0;
    margin-left: 0;
  }

  .latest-jobs-text {
    margin-top: 0 !important;
    padding-top: 10px !important;
  }
}
.mobile-job-view-container {
    width: calc(100% - 90px);
}
.job-description-job-board {
    font-weight: 400;
    font-size: 16px;
