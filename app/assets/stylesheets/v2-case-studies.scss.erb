}

.v2-case-section-1{
  height: 410px;
  width: 100vw;
  z-index: 1;

  h1{
    text-align: center;
    font: normal normal bold 60px/65px Roboto;
    letter-spacing: 0px;
    color: #FAFAFC;
    opacity: 1;
    margin-top: 60px;

    .studies-img{

    }
  }

  p{
    text-align: left;
    font: normal normal 400 24px/30px Proxima Nova;
    letter-spacing: 0px;
    color: #FFFFFF;
    opacity: 1;
    max-width: 839px;
  }

  .case-study{
    height: 72px;
    color: #FAFAFC;
    font: normal normal bold 60px/65px Roboto;
    letter-spacing: 0;
    position: relative;
//    background: transparent url("<%#= asset_path 'v2/casestudies/Group 4548.png' %>") 0% 0% no-repeat padding-box;
    &:after {
      content: '';
      display: block;
      height: 15px;
      width: 15px;
      border-radius: 100px;
      background: #6BCBB8;
      position: absolute;
      top: 4px;
      left: 275px;
      @media only screen and (max-width: 600px) {
        display: none;
      }
    }
  }
}

.case-info{
  width: 100vw;
  height: 500px;
  background: #EBF5FC 0% 0% no-repeat padding-box;

  &.even{
    background: transparent linear-gradient(130deg, #21145F 0%, #008080 100%) 0% 0% no-repeat padding-box;
  }

  .left-flair-logo{
    position: absolute;
    width: 836px;
    height: 806px;
    left: -285px;
    z-index: 1;
    background: transparent url("<%= asset_path 'v2/casestudies/Group 4517.png' %>") 0% 0% no-repeat padding-box;
    /*opacity: 0.15;*/
  }

  .cs-content{
    /*margin-left: 612px;*/

    h5, h3{
      text-align: left;
      font: normal normal bold 25px/33px Roboto;
      letter-spacing: 0px;
      color: #6BCBB8;
      opacity: 1;
      line-height: 18px;
    }

    h5{
      font: normal normal bold 20px/50px Roboto;
      line-height: 20px;
      margin-top: 2px;
      margin-bottom: 0px;
    }

    h3{
      font: normal normal bold 45px/50px Roboto;
      color: #FAFAFC;
      line-height: 45px;
    }

    p{
      text-align: left;
      font: normal normal 400 18px/25px Proxima Nova;
      letter-spacing: 0px;
      color: #FFFFFF;
      opacity: 1;
      margin-bottom: 0px;
      max-width: 670px;
    }

    a{
      text-align: left;
      text-decoration: underline;
      font: normal normal bold 20px/24px Proxima Nova;
      letter-spacing: 0px;
      color: #6BCBB8;
      opacity: 1;
    }

    .cs-title{ height: 90px; }
    .border-line{
      width: 8px;
      /*height: 90px;*/
      /*height: 100%;*/
      background: #6BCBB8 0% 0% no-repeat padding-box;
      border-radius: 4px;
      opacity: 1;
    }

    .violet{
      color: #21145F;
    }

    .white-ish{
      color: #FAFAFC;
    }
  }

  .cs-image{
    width: 500px;
    height: 400px;

    &.logistics{
      background: transparent url("<%= asset_path 'v2/casestudies/logistics-cs.png' %>") 0% 0% no-repeat padding-box;
    }

    &.promo{
      background: transparent url("<%= asset_path 'v2/casestudies/promo-cs.png' %>") 0% 0% no-repeat padding-box;
    }
    &.hospitality{
      background: transparent url("<%= asset_path 'v2/casestudies/hospitality-cs.png' %>") 0% 0% no-repeat padding-box;
    }
    &.sports{
      background: transparent url("<%= asset_path 'v2/casestudies/sports-cs.png' %>") 0% 0% no-repeat padding-box;
    }
    &.events{
      background: transparent url("<%= asset_path 'v2/casestudies/events-cs.png' %>") 0% 0% no-repeat padding-box;
    }
    &.commercial{
      background: transparent url("<%= asset_path 'v2/casestudies/commercial-cs.png' %>") 0% 0% no-repeat padding-box;
    }
  }
}

