.how-it-works-nav{
  // position: absolute;
  height: 236px;
  list-style: none;
  margin-top: -80px;
  padding: 0;

  .step-box{
    position: relative;
    width: 330px;

    .up, .down{
      height: 50%;
    }

    &.first{
      background: #25215D 0% 0% no-repeat padding-box;
      border-radius: 20px 0px 0px 20px;
      opacity: 1;
      .box-title{
        margin-left: 38px;
      }
