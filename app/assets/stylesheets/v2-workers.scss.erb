    &.third{
      background: #6BCBB8 0% 0% no-repeat padding-box;
      border-radius: 0;
      opacity: 1;
      padding: 30px 10px;
      .box-title{
        margin-left: 45px;
      }
    }

    &.fourth{
      background: #8D7BAC 0% 0% no-repeat padding-box;
      border-radius: 0px 20px 20px 0px;
      opacity: 1;
      .box-title{
        margin-left: 30px;
      }
    }

    .step-number{
      font: normal normal bold 107px/86px Roboto;
      position: absolute;
      top: 15px;
      right: 10px;
    }

    .box-title{
      font: normal normal 700 50px/55px Roboto;
      padding-bottom: 8px;
    }

    .box-content{
      font: normal normal 400 18px/30px Proxima Nova;
      margin-top: 0px;
    }
  }
}

