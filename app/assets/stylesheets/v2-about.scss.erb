    }
  }

  .core-container{
    /*width: 100vw;*/
    /*height: 775px;*/
    height: 910px;

    .floating-flair-logo{
      position: absolute;
      width: 942px;
      height: 910px;
      background: transparent url("<%= asset_path 'v2/flair-logo-green.png' %>") 0% 0% no-repeat padding-box;
      /*opacity: 0.22;*/
      /*left: -10rem;*/
      margin-left: -19rem;
    }

    .team-container{
      width: 634px;
      z-index: 0;
      margin-bottom: 80px;
      margin-left: -2rem;

      .img-col{
        width: 198px;
        margin-right: 3%;

        .img-box{
          width: 100%;
          height: 220px;
          transition: .5s ease;
          backface-visibility: hidden;
          position: relative;
          /*border: 1px solid;*/
          z-index: 6;
          cursor: pointer;

          &:nth-child(n+2){
            margin-top: 10%;
          }

          .overlay{
            background: transparent linear-gradient(180deg, #6BCBB8 0%, #21145F 100%) 0% 0% no-repeat padding-box;
            opacity: 0;
            position: absolute;
            width: inherit;
            height: inherit;
            top: 0;
            left: 0;
            transition: all 300ms ease;
            z-index: 5;
          }

          .details{
            opacity: 0;
            transition: all 400ms ease;
            z-index: 10;
            margin-bottom: 17px;
            height: 70px;

            span{
              text-align: center;
              font: normal normal bold 20px/30px Proxima Nova;
              letter-spacing: 0px;
              color: #FFFFFF;
              opacity: 1;

              &:nth-child(n+2){
                font: normal normal 700 16px/20px Proxima Nova;
                color: #fff;
              }
            }
          }

          &:hover .overlay{
            opacity: 0.45;
          }

          &:hover .details{
            opacity: 1;
          }
        }

        &:first-child{
          margin-top: 7rem;

          .img-box{
            &:first-child{
              background: transparent url("<%= asset_path 'v2/aboutus/team/Clare.png' %>") 0% 0% no-repeat padding-box;
            }
            &:nth-child(2){
              background: transparent url("<%= asset_path 'v2/aboutus/Miriam_1.jpeg' %>") 0% 0% no-repeat padding-box;
              background-size: cover;
              background-position-y: -100px;
              background-position-x: center;
            }
          }
        }
        &:nth-child(2){
          .img-box{
            &:first-child{
              background: transparent url("<%= asset_path 'v2/aboutus/team/Elise.png' %>") 0% 0% no-repeat padding-box;
            }
            &:nth-child(2){
              background: transparent url("<%= asset_path 'v2/aboutus/team/Emma.png' %>") 0% 0% no-repeat padding-box;
            }
            &:nth-child(3){
              background: transparent url("<%= asset_path 'v2/aboutus/team/Zoe.png' %>") 0% 0% no-repeat padding-box;
            }
          }
        }
        &:nth-child(3){
          margin-top: 10rem;

          .img-box{
            &:first-child{
              background: transparent url("<%= asset_path 'v2/aboutus/team/Paul.png' %>") 0% 0% no-repeat padding-box;
            }
            &:nth-child(2){
              background: transparent url("<%= asset_path 'v2/aboutus/team/Mark.png' %>") 0% 0% no-repeat padding-box;
            }
            &:nth-child(3){
              background: transparent url("<%= asset_path 'v2/aboutus/team/Dan.png' %>") 0% 0% no-repeat padding-box;
            }
          }
        }
      }
    }

    .team-content{
      z-index: 0;
      margin-left: 10rem;
      /*width: 397px;*/
      p{
        max-width: 397px;
        text-align: left;
      }
    }
  }
