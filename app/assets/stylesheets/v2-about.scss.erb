// stats grid container
.img-boxes-container-grid {
  display: grid;
  width: 100%;
  grid-template-columns: repeat(auto-fill,10%);
  grid-template-rows: 170px 261px;
//  text-align: center;
}
.img-boxs {
  background-size: cover;
  font-family: "Proxima Nova";
  &-1 {
    grid-column-end: span 3;
    background-image: url("<%= asset_path 'v2/aboutus/about-us-1.png' %>");
    padding: 20px 40px;
    &-text-heading {
      font-weight: 600;
      font-size: 16px;
      color: #fff;
    }
    &-spans {
      display: flex;
      color: #fff;
      flex-wrap: wrap;
      margin-top: 18px;
      font-family: "Proxima Nova";
      & span {
        flex: 0 0 38%;
        font-size: 16px;
//        text-align: center;
      }
    }
  }
  &-2 {
    grid-column-end: span 4;
    background-image: url("<%= asset_path 'v2/aboutus/about-us-2.png' %>");
    padding: 15px 30px;
    & div {
      width: 50%;
      font: normal normal bold 18px/21px Proxima Nova;
      letter-spacing: 0px;
      color: #2F353D;
      @media only screen and (max-width: 548px) {
        font: normal normal bold 16px/20px Proxima Nova;
      }
    }
    & h1 {
      font: normal normal bold 90px/128px Proxima Nova;
      letter-spacing: 0px;
      color: #21145F;
      margin: auto;
      width: fit-content;
      @media only screen and (max-width: 548px) {
        font: normal normal bold 60px/65px Proxima Nova;
      }
    }
  }
  &-3 {
    grid-column-end: span 3;
    background-image: url("<%= asset_path 'v2/aboutus/about-us-3.png' %>");
    padding: 15px 30px;
    font-size: 16px;
    font-weight: 700;
    color: #261f57;
    &-heading {
      font: normal normal bold 18px/21px Proxima Nova;
      letter-spacing: 0px;
      color: #21145F;
      width: 134px;
    }
    &-numbers {
      color: #21145F;
      font: normal normal bold 65px/128px Proxima Nova;
      letter-spacing: 0px;
//      margin-top: 36px;
    }
  }
  &-4 {
    grid-column-end: span 3;
    background-image: url("<%= asset_path 'v2/aboutus/about-us-4.png' %>");
    padding: 20px 40px;
    position: relative;
    &-heading-topic {
      color: #21145F;
      font-weight: 700;
      font-size: 14px;
    }
    &-heading-text {
      color: #008080;
      font-size: 30px;
      font-weight: 700;
//      text-align: left;
      margin-left: -30px;
    }
    &-numbers {
      position: absolute;
      bottom: 30px;
      right: 37px;
      width: 155px;
      color: #21145F;
      font: normal normal bold 20px/22px Proxima Nova;
      & span {
        color: #ee7623;
        font: normal normal bold 24px/27px Proxima Nova;

        display: block;
      }
    }
  }
  &-5 {
    grid-column-end: span 3;
    background-image: url("<%= asset_path 'v2/aboutus/about-us-5.png' %>");
    padding: 37px 26px;
    &-heading {
      font: normal normal 600 40px/35px Proxima Nova;
      letter-spacing: 0px;
      color: #FFFFFF;
    }
    &-numbers {
      font: normal normal 600 55px/55px Proxima Nova;
      letter-spacing: 0px;
      color: #FFFFFF;
      margin-top: 20px;
    }
  }
  &-6 {
    grid-column-end: span 4;
  }
}
.img-boxs-6 {
  display: grid;
  width: 100%;
  grid-template-columns: repeat(2, 1fr);
//  grid-auto-columns: 100% 50% 50%;
  @media only screen and (max-width: 548px) {
    grid-template-columns: repeat(1, 1fr);
  }
  & .sub-img-boxes {
    background-size: cover;
    background-repeat: no-repeat;
    &-1 {
      background-image: url("<%= asset_path 'v2/aboutus/about-us-6.png' %>");
      padding-top: 20px;
      &-heading {
        text-align: center;
        font: normal normal bold 18px/21px Proxima Nova;
        letter-spacing: 0px;
        color: #21145F;
        &-main {
          text-align: center;
          font: normal normal bold 35px/55px Proxima Nova;
          letter-spacing: 0px;
          color: #008080;
        }
      }

      &-numbers-row {
          display: flex;
          font: normal normal medium 18px/21px ProximaNova;
          letter-spacing: 0px;
          color: #EE7623;
          opacity: 1;

        justify-content: center;
          & span {
            border-right: 1px solid #008080;
            padding-right: 30px;
            padding-left: 30px;
            &:last-child {
              border-right: none;
            }
          }
      }

      }


    &-2 {
      background-image: url("<%= asset_path 'v2/aboutus/about-us-7.png' %>");
      display: flex;
      justify-content: flex-end;
      padding: 11px 18px;
      &-heading {
        width: 156px;
//        height: 69px;
        text-align: center;
        font: normal normal bold 20px/25px Proxima Nova;
        letter-spacing: 0px;
        align-items: flex-end;
        color: #21145F;
        & span {
          font: normal normal bold 35px/40px Proxima Nova;
        }
      }
    }
    &-3 {
//      background-size: contain;
      background-position-x: -70px;
      text-align: center;
      background-image: url("<%= asset_path 'v2/aboutus/about-us-8.png' %>");
      padding-top: 8px;
      @media only screen and (max-width: 548px) {
        background-image: url("<%= asset_path 'v2/aboutus/about-us-7.png' %>");
        height: 132px;
        background-position-x: 0px;
      }
        &-heading {
        font: normal normal bold 24px/36px Proxima Nova;
        letter-spacing: 0px;
        color: #FFFFFF;
          width: 162px;

          @media only screen and (max-width: 548px) {
            color: #21145F;
            font-size: 18px;
            margin-left: 40px;
            width: 100%;

          }
        }
      &-numbers {
        font: normal normal bold 35px/40px Proxima Nova;
        letter-spacing: 0px;
        color: #FFFFFF;
        width: 162px;

        @media only screen and (max-width: 548px) {
          color: #21145F;
          margin-left: 40px;
          width: 100%;

        }
      }
      &-count-text {
        font: normal normal bold 15px/20px Proxima Nova;
        letter-spacing: 0px;
        color: #FFFFFF;
        margin-left: 80px;
        width: 162px;

        @media only screen and (max-width: 548px) {
          color: #21145F;
          width: 100%;
          margin-left: 120px;
        }
      }
    }
  }
}
.sub-img-boxes-1 {
  grid-column-end: span 2;
}
@media only screen and (max-width: 1100px) and (min-width: 766px) {
  .v2-about-box-content-4 {
    height: 600px;
  }
}
.job-status-position {
  @media only screen and (max-width: 600px) {
    position: absolute;
    top: 6px;
    right: 34px;
    & .max-width-40 {
      max-width: 37px;
    }
    & .badge--event-status span {
      animation: move-to-left 4s linear infinite;
      display: block;
      float: left;
    }
    & .contract-btn, .contract-cancel-button {
      margin-top: 0px;
    }
  }
}
@keyframes move-to-left {
  0% {
    transform: translateX(0);
  }
  100% {
    transform: translateX(-80%);

  }
}
@keyframes marquee {
  0% { left: 0; }
  100% { left: -100%; }
}
