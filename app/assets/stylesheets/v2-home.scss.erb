.slanted-slide{
  text-align: center;
  display: flex;
  justify-content: left;
  position: absolute;
  left: 0;
  cursor: pointer;
}

.container-general{
  width: 600px;
}

.container-general .gallery-wrap{
  width: 100%;
  height: 425px;
  display: flex;
  flex-direction: row;
}
.slanted-container{
  transform: skewX(-10deg);
  flex: 1;
  transition: all 0.5s ease;
  overflow: hidden;
  backface-visibility: hidden;
}
.gallery-wrap .slanted-item{
  flex: 1;
  width: 430px;
  height: 100%;
  background-position: right;
  background-size: auto 100%;
  background-repeat: no-repeat;
  transform: skew(10deg);
  margin-left: -50px;

}

.slanted-active{
  flex: 4;
}

.slanted-active .slanted-item{
  background-position: center !important;
}

.slanted-container:hover{
  flex: 4;
}

.v2-img-carousel .slanted-container:first-child .slanted-item{
  background-image: url("<%= asset_path 'v2/home/images/barguy.png' %>");
  background-position: center;
}
.v2-img-carousel .slanted-container:nth-child(2) .slanted-item{
  background-image: url("<%= asset_path 'v2/home/images/castlefield.png' %>");
}
.v2-img-carousel .slanted-container:nth-child(3) .slanted-item{
  background-image: url("<%= asset_path 'v2/home/images/flairpeople.png' %>");
}
.v2-img-carousel .slanted-container:nth-child(4) .slanted-item{
  background-image: url("<%= asset_path 'v2/home/images/colorrush.png' %>");
  background-position: center;
}
