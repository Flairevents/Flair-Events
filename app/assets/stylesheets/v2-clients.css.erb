.v2-skills-item{
  width: 250px;
  height: 250px;
  position: relative;
  margin-right: 20px;
  cursor: pointer;
  transition: all 300ms ease;
  position: relative;
  overflow: hidden;
  border-radius: 10px;
}

.bg-image{
  width: inherit;
  height: inherit;
  position: absolute;
  background-position: center;
  background-size: 100%;
  z-index: 1;
  transition: all 600ms ease;
}

.v2-skills-item:nth-child(3), .v2-skills-item:last-child {
  margin-right: 0px;
}

.v2-skills-item:nth-child(n+4){
  margin-top: 20px;
}

.v2-skills-item:nth-child(1) .bg-image{
  background-image: url("<%= asset_path 'v2/clients/skills-events.png' %>")
}

.v2-skills-item:nth-child(2) .bg-image{
  background-image: url("<%= asset_path 'v2/clients/skills-sports.png' %>")
}

.v2-skills-item:nth-child(3) .bg-image{
  background-image: url("<%= asset_path 'v2/clients/skills-hospitality.png' %>")
}

.v2-skills-item:nth-child(4) .bg-image{
  background-image: url("<%= asset_path 'v2/clients/skills-logistics.png' %>")
}

.v2-skills-item:nth-child(5) .bg-image{
  background-image: url("<%= asset_path 'v2/clients/skills-commercial.png' %>")
}

.v2-skills-item:nth-child(6) .bg-image{
  background-image: url("<%= asset_path 'v2/clients/skills-promotional.png' %>")
}

.v2-skills-item:hover .bg-image{
  transform: scale(1.1);
}

.v2-skills-item a{
  font: normal normal 700 25px/30px Roboto;
  color: #ffffff !important;
  position: relative;
  display: block;
  text-align: center;
  width: inherit;
  z-index: 4;
  transition: all 300ms ease;
}

.v2-skills-item a::before{
  content: '';
  position: absolute;
  width: 250px;
  height: 250px;
  top: 0;
  left: 0;
  background-color: #28848475;
  z-index: -1;
  transition: all 300ms ease;
  opacity: 0;
  visibility: hidden;
}

.v2-skills-item:hover a{
  transform: translate(0px, -20px);
}

.v2-skills-item:hover a::before {
  opacity: 1;
  visibility: visible;
  top: 20px;
}

.v2-skills-item:nth-child(6):hover a{
  transform: translate(0px, -35px);
}

.v2-skills-item:nth-child(6):hover a::before{
  top: 35px;
}

.overlay-container{
  /*background: transparent linear-gradient(180deg, #0080805E 0%, #008080 100%) 0% 0% no-repeat padding-box;*/
  /*opacity: 0.4;*/
  transition: .5s ease;
  opacity: 0;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  text-align: center;
  width: 250px;
  height: 250px;
}

