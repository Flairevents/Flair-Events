.v2-industry-section-2{
  height: 647px;
  background: #EBF5FC 0% 0% no-repeat padding-box;

  h1{
    text-align: left;
    font: normal normal bold 45px/45px Roboto;
    letter-spacing: 0px;
    color: #21145F;
    opacity: 0.93;
    /*max-width: 530px;*/
  }

  p{
    text-align: left;
    font: normal normal 400 18px/30px Proxima Nova;
    letter-spacing: 0px;
    color: #21145F;
    opacity: 1;
    max-width: 623px;
  }

  ul{
    width: 36.38vw;
    height: 520px;
    list-style: none;
    padding: 0;
    @media only screen and (max-width: 1280px) and (min-width: 900px) {
      width: 40vw;
      min-width: 440px;

    }
    li{
      width: 17.36vw;
      height: 250px;
      /*margin-right: 24px;*/
      margin-bottom: 20px;
      border-radius: 10px;
      /*border: 1px solid;*/
      cursor: pointer;
      transition: all 300ms ease;
      position: relative;
      overflow: hidden;

      &:nth-child(2), &:nth-child(4){ margin-right: 0px; }
      &:nth-child(n+2){
        margin-bottom: 0px;
      }

      .light-overlay{
        width: 17.36vw;
        height: 250px;
        position: absolute;
        top: 0;
        background: #008080;
        opacity: 0.30;
        z-index: 1;
      }

      .flat-overlay {
        position: absolute;
        width: 17.36vw;
        height: 250px;
        background: transparent url("<%= asset_path 'v2/industry/Castlefield crowd.png' %>") 0% 0% no-repeat padding-box;
        /*transform: scale(0);*/
        /*opacity: 0.7;*/
        background-size: cover;
        cursor: pointer;
        transition: all 100ms ease;
        z-index: 2;
        visibility: hidden;
      }

      .circular {
        position: absolute;
        width: 15.06vw;
        height: 217px;
        background: transparent url("<%= asset_path 'v2/clients/Path 3405.png' %>") 0% 0% no-repeat padding-box;
        transform: scale(0);
        opacity: 0.7;
        background-position: center;
        cursor: pointer;
        top: 20px;
        transition: all 300ms ease;
        z-index: 3;
      }

      .type-image{
        width: 17.36vw;
        height: inherit;
        position: absolute;
        background-repeat: no-repeat;
        background-position: center;
        background-size: cover;
        // background-size: cover;
        z-index: 1;
        transition: all 600ms ease;
      }

      .type-desc{
        position: absolute;
        text-align: center;
        font: normal normal 400 18px/25px Proxima Nova;
        color: #FFFFFF;
        opacity: 0;
        top: 55%;
        z-index: 5;
        transition: all 400ms ease;
      }

      a{
        font: normal normal bold 2.22vw/2.29vw Roboto;
        color: #ffffff !important;
        position: relative;
        text-align: center;
        width: inherit;
        z-index: 4;
        transition: all 200ms ease;
        /*visibility: hidden;*/
        text-align: center;

        &::before{
          content: '';
          position: absolute;
          width: 250px;
          height: 250px;
          top: 0;
          left: 0;
          background-color: #28848475;
          z-index: -1;
          transition: all 300ms ease;
          opacity: 0;
          visibility: hidden;
        }
      }

      &:hover .circular{
        transform: scale(1.4);
      }
      &:hover .flat-overlay{
        /*transform: scale(1);*/
        visibility: visible;
      }
      &:hover .type-image{
        transform: scale(1.1);
      }
      &:hover .type-desc{
        opacity: 1;
      }
    }
  }
}

